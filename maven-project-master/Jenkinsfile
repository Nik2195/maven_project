pipeline
{
agent {
  label 'dev'
}

environment{
  NAME = "NIKHIL"
}

tools {
  maven 'maven_test'
}

stages{

    stage ('build')
    {
        steps{
          script {
              dir('maven-project-master/') {
                sh 'mvn clean package'
                echo "hello $NAME"
        }
        }
        }
        post {
        success {
            archiveArtifacts artifacts: '**/target/*.war'
                }
            }
    }

}
}
