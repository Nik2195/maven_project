pipeline
{
agent {
  label 'dev'
}
tools {
  maven 'maven_test'
}

stages{

    stage ('build')
    {
        steps{
          script {
              dir('maven-project-master/') {
                sh 'mvn clean package'
        }
        }
        }
        post {
        success {
            archiveArtifacts artifacts: '**/target/*.war'
                }
            }
    }

}
}
# line 14 15 16 is required as maven is not installed in ubuntu os. For maven to work it should utilize the pom.xml file.
# To locate pom.xml, in the github repository, mention the path of the file.